<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <server>
      <username>__USER__</username>
      <password>__PASS__</password>
      <id>central</id>
    </server>
    <server>
      <username>__USER__</username>
      <password>__PASS__</password>
      <id>snapshots</id>
    </server>
  </servers>
  <profiles>
    <profile>
      <repositories>
        <repository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>central</id>
          <name>bb-maven-repo</name>
          <url>https://atf.intranet.bb.com.br:443/artifactory/bb-maven-repo</url>
        </repository>
        <repository>
          <snapshots />
          <id>snapshots</id>
          <name>bb-maven-repo</name>
          <url>https://atf.intranet.bb.com.br:443/artifactory/bb-maven-repo</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>central</id>
          <name>bb-maven-repo</name>
          <url>https://atf.intranet.bb.com.br:443/artifactory/bb-maven-repo</url>
        </pluginRepository>
        <pluginRepository>
          <snapshots />
          <id>snapshots</id>
          <name>bb-maven-repo</name>
          <url>https://atf.intranet.bb.com.br:443/artifactory/bb-maven-repo</url>
        </pluginRepository>
      </pluginRepositories>
      <id>artifactory</id>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>artifactory</activeProfile>
  </activeProfiles>
</settings>
